      "request": "launch",
      "mainClass": "com.agromercado.eureka.EurekaServerApplication",
      "projectName": "eureka-server",
      "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=64077 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=eureka-server"
    },
    {
      "type": "java",
      "name": "Spring Boot-PedidosApplication<pedidos-service>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "com.agromercado.pedidos.PedidosApplication",
      "projectName": "pedidos-service",
      "args": "",
      "envFile": "${workspaceFolder}/.env"
