{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "AccountsServiceApplication",
      "request": "launch",
      "mainClass": "com.agromercado.accounts.AccountsServiceApplication",
      "projectName": "accounts-service",
      "args": "--spring.profiles.active=dev",
      "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=51800 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=accounts-service"
    },
    {
      "type": "java",
      "name": "ApiGatewayApplication",
      "request": "launch",
      "mainClass": "com.agromercado.gateway.ApiGatewayApplication",
      "projectName": "api-gateway",
      "args": "--spring.profiles.active=dev"
    },
    {
      "type": "java",
      "name": "ProductosApplication",
      "request": "launch",
      "mainClass": "com.agromercado.productos.ProductosApplication",
      "projectName": "productos-service",
      "args": "--spring.profiles.active=dev",
      "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=53898 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=productos-service"
    },
    {
      "type": "java",
      "name": "PedidosServiceApplication",
      "request": "launch",
